{
  "name": "nimbus-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "yarn install && yarn dev",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
