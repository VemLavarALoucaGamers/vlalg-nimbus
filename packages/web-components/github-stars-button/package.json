{
	"name": "@vlalg-nimbus/github-stars-button",
	"description": "A button that shows the number of stars of a Github repository",
	"version": "1.0.2",
	"type": "module",
	"main": "./dist/github-stars-button.min.umd.js",
	"module": "./dist/github-stars-button.min.es.js",
	"exports": {
		".": {
			"import": "./dist/github-stars-button.min.es.js",
			"require": "./dist/github-stars-button.min.umd.js"
		},
		"./package.json": "./package.json"
	},
	"sideEffects": true,
	"files": [
		"dist"
	],
	"scripts": {
		"dev:server": "live-server --entry-file=./modules/index.html --verbose",
		"prod:minify": "cross-env NODE_ENV=production yarn rollup",
		"dev:watch-prod": "cross-env NODE_ENV=production yarn rollup:watch",
		"dev": "concurrently \"yarn clean\" \"yarn dev:watch-prod\" \"yarn dev:server\"",
		"rollup": "rollup --config",
		"rollup:watch": "rollup --config --watch",
		"clean": "rimraf dist",
		"lint": "eslint ./modules --ext .js --fix",
		"npm:publish": "yarn clean && yarn lint && yarn prod:minify && npm login && npm publish --access=public"
	},
	"devDependencies": {
		"@babel/core": "^7.22.11",
		"@babel/eslint-parser": "^7.22.11",
		"@babel/plugin-external-helpers": "^7.22.5",
		"@babel/preset-env": "^7.22.14",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-eslint": "^9.0.4",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.48.0",
		"live-server": "^1.2.2",
		"rimraf": "^5.0.5",
		"rollup": "^3.28.1",
		"rollup-plugin-license": "^3.1.0",
		"rollup-plugin-uglify": "^6.0.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/VemLavarALoucaGamers/vlalg-nimbus"
	},
	"bugs": {
		"url": "https://github.com/VemLavarALoucaGamers/vlalg-nimbus/issues"
	},
	"homepage": "https://nimbus.tec.br",
	"license": "MIT",
	"keywords": [
		"module",
		"lib",
		"web",
		"component",
		"web component",
		"html",
		"css",
		"js",
		"github",
		"github star",
		"github stars"
	],
	"author": "Antonio and Felipe <Vem Lavar a Louca Gamers> (https://vemlavaralouca.com.br)",
	"contributors": [
		"TonhaoSantos (https://github.com/TonhaoSantos)",
		"Felipecss (https://github.com/lipecss)"
	]
}
