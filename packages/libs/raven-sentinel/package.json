		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.48.0",
		"rollup": "^3.28.1",
		"rollup-plugin-uglify": "^6.0.4"
