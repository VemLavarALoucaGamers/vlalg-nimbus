{
  "name": "vlalg-nimbus",
  "description": "VLALG Nimbus",
  "version": "2.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "install-all": "yarn cache clean && yarn workspaces foreach --all run yarn install",
    "deploy:playground": "yarn workspace nimbus-playground deploy:vercel",
    "clean:node-modules": "find . -type d -name 'node_modules' -exec rm -rf {} + && rm -f yarn.lock"
  },
  "workspaces": {
    "packages": [
      "apps/**/*",
      "packages/**/*"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VemLavarALoucaGamers/vlalg-nimbus"
  },
  "bugs": {
    "url": "https://github.com/VemLavarALoucaGamers/vlalg-nimbus/issues"
  },
  "homepage": "https://nimbus.tec.br",
  "license": "MIT",
  "keywords": [
    "vuejs",
    "vue js",
    "vue plugin",
    "components",
    "component",
    "vue components",
    "vuejs components",
    "vue js components",
    "modules",
    "js lib",
    "javascript lib"
  ],
  "author": "Antonio and Felipe <Vem Lavar a Louca Gamers> (https://vemlavaralouca.com.br)",
  "contributors": [
    "TonhaoSantos (https://github.com/TonhaoSantos)",
    "Felipecss (https://github.com/lipecss)"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.5.1",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0"
  }
}
